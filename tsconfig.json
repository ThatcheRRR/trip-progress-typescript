{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist/",
    "strictNullChecks": false,
  },
  "exclude": ["node_modules"],
  "include": ["src"]
}
